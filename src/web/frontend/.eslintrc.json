{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "indent": ["error", 4],
        "semi": [2, "never"],
        "no-console": 0,
        "react/jsx-indent-props": [2, 4],
        "react/jsx-indent": [2, 4]
    },
    "globals": {
        "fetch": true,
        "document": true,
        "FileReader": true
    }
}
